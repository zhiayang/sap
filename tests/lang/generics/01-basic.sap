global var ppp = builtin::println;

fn asdf() -> (any) -> void
{
	return ppp;
}

fn f1[T](a: T)
{
	ppp(f"f1(a)");
	asdf()(a);
}

fn f2[T, U](a: T, b: U)
{
	asdf()(f"a={a}, b={b}");
}

